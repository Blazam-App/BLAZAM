@inherits AppComponentBase

@if (Model.Id > 0)
{
    <MudExpansionPanel Text="@Model.Name">
        <EditAccessLevel Model=Model />
    </MudExpansionPanel>
}
else
{
    <EditAccessLevel Model=Model />

}


@code {
#nullable disable warnings
    bool collapseVisible = false;
    [Parameter]
    public AccessLevel Model { get; set; }


}